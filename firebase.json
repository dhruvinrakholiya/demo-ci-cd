{
  "functions": {
    "rewrites": [
      {
        "source": "/functions",
        "function": "expressApp"
      }
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "functions",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "expressApp"
      }
      
    ]
  }
}
